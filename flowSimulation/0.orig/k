FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../../include/settings" // It's not "../include/settings" because it will be run from processor* folders, which need on more ../

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;  // Parameters imported from DragAndDownForcePractice/meshGenerator/include/settings

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    //- Define inlet conditions
    inlet
    {
        type  fixedValue;
        value $internalField; // Parameters defined in this file
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField; // Parameters defined in this file
        value           $internalField; // Parameters defined in this file
    }

    lowerWall
    {
        type            kqRWallFunction;
        value           $internalField; // Parameters defined in this file
    }

    $geometry_boundary  // Parameters imported from DragAndDownForcePractice/meshGenerator/include/settings sourced via DragAndDownForcePractice/flowSimulation/include/setting
    {
        type            kqRWallFunction;
        value           $internalField; // Parameters defined in this file
    }

    upperWall
    {
        type slip;
    }

    frontAndBack
    {
        type slip;
    }
}


